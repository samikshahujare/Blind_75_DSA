class Solution:
    def findMin(self, nums):
        low, high = 0, len(nums) - 1

        while low < high:
            mid = (low + high) // 2

            # If mid element is bigger than the last element,
            # the minimum is definitely on the right side.
            if nums[mid] > nums[high]:
                low = mid + 1

            # Otherwise, the minimum is on the left side (including mid)
            else:
                high = mid

        return nums[low]
